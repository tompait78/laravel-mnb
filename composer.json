{
  "name": "szunisoft/laravel-mnb",
  "license": "MIT",
  "description": "Laravel MNB (Magyar Nemzeti Bank)",
  "keywords": [
    "mnb",
    "magyar",
    "nemzeti",
    "bank",
    "currency",
    "exchange",
    "rate",
    "laravel",
    "package"
  ],
  "authors": [
    {
      "name": "Forg√°cs Bence",
      "email": "sykorax93@gmail.com"
    }
  ],
  "require": {
    "php": "^7|^8",
    "szunisoft/mnb": "dev-master",
    "illuminate/support": "^6|^7|^8|^9",
    "illuminate/cache" : "^6|^7|^8|^9"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "1.3.*",
    "orchestra/testbench": "^5.1.0"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "SzuniSoft\\Mnb\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SzuniSoft\\Mnb\\Laravel\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SzuniSoft\\Mnb\\Laravel\\MnbServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tompait78/php-mnb"
    }
  ]
}
